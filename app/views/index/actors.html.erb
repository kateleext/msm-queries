<h2>List of all actors</h2>
<hr>
<table border="1">
<%@list_of_actors.each do |actor|%>
<tr>
    <td>
      <%=actor.id%>  
    </td>

    <td>
      <img src=<%=actor.image%> style = "width:250px">
    </td>

    <td>
      <%=actor.name%>
    </td>

    <td>
      <%=actor.dob%>
    </td>

    <td>
      <a href=<%="/actors/#{actor.id}"%>>
        Show details
      </a>
    </td>
  </tr>
<%end%>
</table>
